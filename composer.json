{
    "name": "convenia/revisionable",
    "license": "MIT",
    "description": "Keep a revision history for your eloquent models without thinking",
    "keywords": ["model", "laravel", "revision", "history"],
    "homepage": "http://github.com/convenia/revisionable",
    "authors": [        
        {
            "name": "Victor Ventura",
            "email": "euventura@gmail.com"
        },
        {
            "name": "Eduardo Bizarro",
            "email": "edbizarro@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/convenia/revisionable/issues",
        "source": "https://github.com/convenia/revisionable"
    },
    "require": {
        "php": ">=5.6",
        "illuminate/support": "~5.2|~5.3|~5.4"
    },
    "require-dev": {
        "codacy/coverage": "^1.2",        
        "mockery/mockery": "^0.9.9",
        "orchestra/testbench": "~3.0",
        "phpunit/phpunit": "^5.0|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Convenia\\Revisionable\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Convenia\\Revisionable\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit"        
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
